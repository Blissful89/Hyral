{
  "name": "hyral",
  "version": "0.1.0",
  "description": "Hypermedia oriented Resource Api Layer",
  "main": "lib",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest --watchAll",
    "lint": "eslint packages/**/src",
    "fix": ".eslint --fix packages/**/src",
    "prebuild": "npm run lint",
    "build": "lerna exec --parallel 'BABEL_ENV=production babel src --root-mode upward --out-dir lib --source-maps --extensions .js --delete-dir-on-start --no-comments",
    "prerelease": "npm run build",
    "release": "lerna publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SyneticNL/Hyral.git"
  },
  "keywords": [
    "Api",
    "Rest",
    "JsonApi"
  ],
  "author": "Synetic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SyneticNL/Hyral/issues"
  },
  "homepage": "https://github.com/SyneticNL/Hyral#readme",
  "peerDependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "lerna": "^3.13.4",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/runtime": "^7.4.4",
    "babel-jest": "^24.7.1",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-minify": "^0.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "jest": "^24.7.1",
    "jest-json-schema": "^2.0.2",
    "jest-mock-axios": "^3.0.0"
  }
}
